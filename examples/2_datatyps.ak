# datatypes

let int = 4;
let float = 3.5;
let string = "something";
let bool = false; # or `true`
let null_ = null;

let list = [1, 2, 3, 4]; # or `1..4`
println(list[0]); # output: `1`

let tuple = (1, 2);
println(list[1]); # output: `2`

let object = {
    key: "value"
};
println(object.key); # output: `value`
